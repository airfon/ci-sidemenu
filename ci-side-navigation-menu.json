{
  "name": "Cost Insights - Side Navigation Menu",
  "description": "Cost Insights - Side Navigation Menu - Sliding Menu",
  "eula": "",
  "vendor": "",
  "license": "",
  "id": "com.sap.side.navigation.menu",
  "newInstancePrefix": "SideNavigationMenu",
  "version": "0.8.0",
  "icon": "",
  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-sap-ci-side-navigation-menu",
      "url": "https://sac-dev-cw.novartis.net/cw/dev/side-navigation-menu/ci-side-navigation-menu.js",
      "integrity": "",
      "ignoreIntegrity" : true
    }
  ],
  "properties": {
    "URLParameter": {
      "type": "string",
      "description": "The URL Parameter",
      "default": ""
    },
    "AccessParameter": {
      "type": "any",
      "description": "Access Parameter",
      "default": ""
    }
  },
  "methods": {
    "getID": {
      "returnType": "string",
      "description": "Return ID of option selected"
    },
    "getParameter": {
      "description": "Get the Parameter from the URL",
      "returnType": "string"
    },
    "setParameter": {
      "description": "Sets the Parameter String to the URL",
      "parameters": [
        {
          "name": "newParameter",
          "type": "string",
          "description": "Parameter String"
        }
      ],
      "body": "this.URLParameter = newParameter;"
    },

    "setAccess": {
      "description": "Sets the Access of the User",
      "parameters": [
        {
          "name": "newAccess",
          "type": "any",
          "description": "Access String"
        }
      ],
      "body": "this.AccessParameter = newAccess;"
    }
  },
  "events": {
    "onClick": {
      "description": "Returns the option selected."
    },
    "onOpen": {
      "description": "Event on the user opens the Menu"
    },
    "onClose": {
      "description": "Event on the user closes the Menu"
    }
  }
}